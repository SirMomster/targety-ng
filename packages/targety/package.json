{
    "name": "targety",
    "version": "0.1.10",
    "description": "AWS routing library for AWS Lambda, routing straight to your target",
    "license": "MIT",
    "main": "lib/index.js",
    "publishConfig": {
        "registry": "https://registry.npmjs.com/"
    },
    "repository": {
        "url": "https://github.com/becloudway/targety"
    },
    "scripts": {
        "build": "rm -rf ./lib && tsc",
        "test": "cd .. && npm run test",
        "docs": "typedoc ./src/index.ts --name targety --out ./docs",
        "prepublishOnly": "npm run docs && npm install && yarn run build && npm prune --production"
    },
    "directories": {
        "lib": "./lib"
    },
    "dependencies": {
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie": "^0.4.0",
        "lodash.get": "^4.4.2",
        "lodash.tostring": "^4.1.4",
        "pino": "^6.13.0",
        "reflect-metadata": "^0.1.13",
        "url-parse": "^1.4.7",
        "yn": "^4.0.0"
    },
    "devDependencies": {
        "@types/cookie": "^0.3.3",
        "@types/lodash.get": "^4.4.6",
        "@types/lodash.tostring": "^4.1.6",
        "@types/node": "^12.7.9",
        "@types/pino": "^6.3.10",
        "@types/url-parse": "^1.4.3",
        "dotenv": "^8.1.0",
        "typedoc": "^0.21.4"
    }
}
